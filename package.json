{
  "name": "ai-kawaii-terminal",
  "version": "1.0.0",
  "description": "AI Character Chat Terminal App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/react": "^18.0.29",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "@pixiv/three-vrm": "^1.0.10",
    "@types/node": "^18.15.10",
    "@types/react-dom": "^18.0.11",
    "@types/three": "^0.149.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "next": "^13.2.4",
    "node-pty": "^1.0.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17",
    "three": "^0.149.0",
    "typescript": "^5.0.2"
  },
  "build": {
    "appId": "com.ai-kawaii.terminal",
    "productName": "AI Kawaii Terminal",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "src/**/*",
      "assets/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
